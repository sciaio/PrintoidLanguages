    <string name="alert_network_unavailable_title">Mit keinem Netzwerk verbunden.</string>
    <string name="alert_network_unavailable_message">Dein Gerät ist mit keinem Netzwerk verbunden. Printoid kann deshalb nicht auf deinen OctoPrint Server zugreifen.</string>

    <!-- PRINTER CONNECTION -->
    <string name="connector_printer_connection_title">Druckerverbindung…</string>
    <string name="connector_printer_connection_msg">Printoid verbindet sich mit Ihrem 3D-Drucker, bitte warten Sie…</string>

