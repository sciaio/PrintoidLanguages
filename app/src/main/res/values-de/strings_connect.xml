    <string name="connector_connection_step_3" tools:ignore="TypographyFractions">Schritt 3/3</string>

    <!-- CONNECTOR STEP 1 (PING) -->
    <string name="connector_connecting_title">Verbinden - Schritt 1 (Ping)</string>
    <string name="connector_connecting_msg">Ereichbarkeit des OctoPrint Servers überprüfen von %1$s&#8230;\n\nAnschrift: %2$s</string>
    <string name="connector_connecting_failed_title">Fehler bei Schritt 1 (Ping)</string>
    <string name="connector_connecting_failed_msg"><![CDATA[Printoid konnte deinen OctoPrint Server nicht erreichen von %1$s. Bitte prüfe dein lokales Netzwerk und die eingestellte IP.<br><br><b>Zusatzinformationen:</b><br><br>%2$s]]></string>

    <!-- CONNECTOR STEP 2 (CHECK IP) -->
    <string name="connector_communicating_title">Verbinden - Schritt 2 (IP Test)</string>
    <string name="connector_communicating_msg">Verbindung mit OctoPrint Server herstellen von %1$s&#8230;\n\nAnschrift: %2$s</string>
    <string name="connector_communication_failed_title">Fehler bei Schritt 2 (IP Test)</string>
    <string name="connector_communication_failed_msg"><![CDATA[Printoid konnte die Verbindung mit deinem OctoPrint Server nicht herstellen von %1$s. Bitte prüfe deine Netzwerkverbindung und Einstellungen<br><br><b>Zusatzinformationen:</b><br><br>%2$s]]></string>

    <!-- CONNECTOR STEP 3 (CHECK API) -->
    <string name="connector_testing_title">Verbinden - Schritt 3 (API Test)</string>
    <string name="connector_testing_msg">Printoid kommuniziert mit deinem Drucker von %1$s&#8230;\n\nAnschrift: %2$s</string>
    <string name="connector_test_failed_title">Fehler bei Schritt 3 (API Test)</string>
    <string name="connector_test_failed_msg"><![CDATA[Printoid konnte keine Befehle an deinen Drucker senden von %1$s. Bitte kontaktiere den Entwickler von Printoid.<br><br><b>Zusatzinformationen:</b><br><br>%2$s]]></string>

    <!-- EXTRA MSG: VPN ACTIVE-->
    <string name="connector_failed_vpn">\n\nHinweis: Printoid hat ein aktives VPN auf Ihrem Gerät erkannt, das die Verbindung zu Ihrem OctoPrint Server beeinträchtigen kann.</string>

    <!-- CONNECTOR STATUS -->
    <string name="connector_communication_established_title">Verbindung hergestellt</string>
    <string name="connector_communication_established_msg">Printoid kann nun mit deinem OctoPrint Server kommunizieren.\nSie können Ihren Server vom linken Menü aus mit dem Drucker verbinden.</string>
    <string name="octoprint_connection_lost">Verbindung mit OctoPrint abgebrochen</string>
    <string name="connector_profile_not_completed_title">OctoPrint Profil nicht vollständig</string>
    <string name="connector_profile_not_completed_message">Das OctoPrint Profil das du einstellt hast scheint unvollständig zu sein.\nBitte korrigiere die Einstellungen, bevor du die Verbindung herstellst.</string>
    <string name="connector_communication_canceled">Verbindung abgebrochen</string>
    <string name="connector_communication_established_with">Verbindung hergestellt: %s</string>
    <string name="connector_communication_canceled_with">Verbindung abgebrochen: %s</string>

