    <string name="connector_error_connectivity_response">No connectivity or signal too bad, please check your current network</string>
    <string name="connector_error_invalid_api_key">Invalid API key</string>
    <string name="connector_error_missing_api_key">Missing API key</string>
    <string name="connector_error_unauthorized">Authorization failed, may you should activate the connection with basic auth in Printoid\'s settings or verify your credentials</string>
    <string name="connector_error_http_error">HTTP request error: %2$d (%1$s)</string>
    <string name="connector_error_parsing_exception">Unknown API format, please contact the Printoid\'s developer</string>

    <!-- NETWORK ISSUE -->
    <string name="alert_network_connecting_title">Connecting to a network…</string>
    <string name="alert_network_connecting_message">Your device is currently connecting to a network, please retry the connection in a few seconds</string>
    <string name="alert_network_unavailable_title">Not connected to any network</string>
    <string name="alert_network_unavailable_message">Your device is not connected to any network, Printoid can\'t access to your OctoPrint server</string>

